<?xml version="1.0" encoding="utf-8"?>
<Definition xmlns="http://www.hhdsoftware.com/schemas/langset/1.0.xsd">
  <Enumeration key="FileOpenProcess" description="Command: File.Open.Process" revision="1">
    <Item text="Open P&amp;rocess…"/>
    <Item text="Open process memory for reading and writing"/>
  </Enumeration>

  <Enumeration key="GoToProcess" description="Command: Edit.Goto process" revision="1">
    <Item text="Go to… (in Process)"/>
    <Item text="Jump to the given section in the edited process memory"/>
  </Enumeration>

  <Enumeration key="BlockType" description="Memory block types" revision="1">
    <Item text="Unknown"/>
    <Item text="Image"/>
    <Item text="Mapped"/>
    <Item text="Private"/>
  </Enumeration>

  <Enumeration key="AllocationType" description="Memory block allocation types" revision="1">
    <Item text="Unknown"/>
    <Item text="Committed"/>
    <Item text="Free"/>
    <Item text="Reserved"/>
  </Enumeration>

  <Enumeration key="PageAccess" description="Memory block access" revision="1">
    <Item text="No Access"/>
    <Item text="Read Only"/>
    <Item text="Read, Write"/>
    <Item text="Copy on Write"/>
    <Item text="Execute"/>
    <Item text="Execute, Read"/>
    <Item text="Execute, Read, Write"/>
    <Item text="Execute, Copy on Write"/>
    <Item text="Protected (Guarded)"/>
    <Item text="No Cache"/>
  </Enumeration>

  <Enumeration key="FeatureDesc" description="Open Process feature" revision="1">
    <Item text="Open Process"/>
    <Item text="Open Process feature allows you to open a selected process' memory address space for viewing and editing."/>
  </Enumeration>

  <Message key="PrivilegeNotGained" description="Option: Limited set of processes in processes list" revision="1" value="Limited set of processes in processes list"/>
  <Enumeration key="Task" description="Jump List Item: Open Process" revision="1">
    <Item text="Open process"/>
    <Item text="Open process memory for viewing and modification"/>
  </Enumeration>

  <Message key="RegionInfo" description="Selected region info (both strings must be the same!)" revision="1" value="Region base: {base} ({base:2})\rRegion end: {end} ({end:2})\rRegion size: {size} ({size:2})\rType: {type}\rState: {state}\rFlags: {flags}">
    <Param name="base" format="b016"/>
    <Param name="base:2" format="l"/>
    <Param name="end" format="b016"/>
    <Param name="end:2" format="l"/>
    <Param name="size" format="b016"/>
    <Param name="size:2" format="l"/>
    <Param name="type"/>
    <Param name="state"/>
    <Param name="flags"/>
  </Message>

  <Message key="Unknown" description="Unknown" revision="1"/>
  <Enumeration key="DisplayNotification" description="Notification: Limited Open Process Functionality" revision="1">
    <Item text="Limited Open Process Functionality"/>
    <Item text="Warning: Hex Editor Neo lacks the required privileges to access processes running on your system. It will only be able to connect to a small subset of processes running on your computer. Run Hex Editor Neo as an elevated user with administrative access to be able to edit any process' memory address space."/>
  </Enumeration>

  <Enumeration key="Tabs" description="Open Process window tabs" revision="1">
    <Item text="Blocks"/>
    <Item text="Modules"/>
  </Enumeration>
  <Message key="No process selected" description="No process selected" revision="1"/>
  <Message key="Specify a range using a list of blocks or a list of modules." description="Specify a range using a list of blocks or a list of modules" revision="1"/>
  <Message key="Either start or end is too big. Please enter correct values." description="Either start or end is too big. Please enter correct values" revision="1"/>

  <Enumeration key="ProcessColumns" description="Process list columns" revision="1">
    <Item text="ID"/>
    <Item text="Name"/>
  </Enumeration>

  <Enumeration key="HeapColumns" description="Heap columns" revision="1">
    <Item text="Address"/>
    <Item text="Size"/>
    <Item text="Type"/>
    <Item text="State"/>
    <Item text="Flags"/>
    <Item text="Module"/>
  </Enumeration>
  <Enumeration key="ModulesColumns" description="Modules columns" revision="1">
    <Item text="Name"/>
    <Item text="Address"/>
    <Item text="Size"/>
  </Enumeration>
  <Message key="Multiple selection is allowed only when sorting by Address. Click on first column's header to sort blocks." description="Multiple selection is allowed only when sorting by Address. Click on first column's header to sort blocks" revision="1"/>
  <Message key="ProcessDescription" description="Process description" revision="1" value="Process: {process}, Blocks: {blocks}, Modules: {modules}">
    <Param name="process"/>
    <Param name="blocks" format="l"/>
    <Param name="modules" format="l"/>
  </Message>
  <Message key="N/A" description="Process name is unknown" revision="1"/>

  <!-- Dialog boxes -->
  <Message key="Open Process" description="Open Process Window title" revision="1"/>
  <Message key="Set Colors…" description="Button: Set Colors…" revision="1"/>
  <Message key="Display decimal values" description="Checkbox: Display decimal values" revision="1"/>
  <Message key="Logarithmic scale" description="Checkbox: Logarithmic scale" revision="1"/>
  <Message key="Start:" description="Field: Start" revision="1"/>
  <Message key="End:" description="Field: End" revision="1"/>
  <Message key="Size:" description="Field: Size" revision="1"/>
  <Message key="Show All Processes" description="Button: Show All Processes" revision="1"/>
  <Message key="Refresh" description="Button: Refresh" revision="1"/>

  <!-- Color dialog -->
  <Message key="Color Selection" description="Color Selection window title" revision="1"/>
  <Message key="No Access/Guard" description="Field: No Access/Guard" revision="1" value="No Access/Guard:"/>
  <Message key="Read Only/Execute" description="Field: Read Only/Execute" revision="1" value="Read Only/Execute:"/>
  <Message key="Read/Write" description="Field: Read/Write" revision="1" value="Read/Write:"/>
  <Message key="Copy on Write" description="Field: Copy on Write" revision="1" value="Copy on Write:"/>
  <Message key="Free" description="Field: Free" revision="1" value="Free:"/>
</Definition>
